@model IEnumerable<GooglePlus.Data.Model.Photo>

@{
    ViewBag.Title = "Photos";
}

<h2>Photos</h2>

<p>
    @Html.ActionLink("Create New", "CreatePhoto")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Src)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Comment)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Url)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Created)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.googleId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Src)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Comment)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Url)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Created)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.googleId)
        </td>
    </tr>
}

</table>
